#include <iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int x;
    cin>>x;
   
    int i=n-1;
    while(i>0){
        if(x==arr[i]){
            cout<<i;
        break;
        }
        else
            cout<<"-1";
    }
    return 0;
}
